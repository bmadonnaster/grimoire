        WEB_SITE=http://www.ghostscript.com/
   SOURCE_URL[0]=https://github.com/ArtifexSoftware/ghostpdl-downloads/releases/download/gs${VERSION//./}/${SOURCE}
      LICENSE[0]="AGPL"
