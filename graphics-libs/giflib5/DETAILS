          SOURCE="${SPELLX}-${VERSION}.tar.gz"
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELLX-$VERSION
   SOURCE_URL[0]="https://downloads.sourceforge.net/${SPELLX}/${SOURCE}"
      LICENSE[0]=MIT
        WEB_SITE="http://giflib.sourceforge.net/"
