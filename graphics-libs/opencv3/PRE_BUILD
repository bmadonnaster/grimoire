default_pre_build &&
cd $SOURCE_DIRECTORY &&
unpack_file 3 &&
if ! list_find "$CV_CONTRIB" none ;then
 cd $BUILD_DIRECTORY  &&
 unpack_file 2 &&
 cd $SOURCE_DIRECTORY &&
 mkdir contrib &&
 cd contrib    &&
 for FF in $CV_CONTRIB;do
   ln -sf $BUILD_DIRECTORY/opencv_contrib-$VERSION/modules/$FF
 done
fi
