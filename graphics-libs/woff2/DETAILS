           SPELL=woff2
         VERSION=1.0.2
          SOURCE="$SPELL-$VERSION.tar.gz"
     SOURCE_HASH=sha512:c788bba1530aec463e755e901f9342f4b599e3a07f54645fef1dc388ab5d5c30625535e5dd38e9e792e04a640574baa50eeefb6b7338ab403755f4a4e0c3044d
