           SPELL=freetype2
         VERSION=2.8.1
  SECURITY_PATCH=11
          SOURCE=freetype-$VERSION.tar.bz2
         SOURCE2=$SOURCE.sig
SOURCE_DIRECTORY="$BUILD_DIRECTORY/freetype-$VERSION"
   SOURCE_URL[0]=http://downloads.sourceforge.net/sourceforge/freetype/$SOURCE
   SOURCE_URL[1]=http://download.savannah.gnu.org/releases/freetype/$SOURCE
  SOURCE2_URL[0]=$SOURCE_URL.sig
  SOURCE2_IGNORE=signature
      SOURCE_GPG=gnu.gpg:$SOURCE2:UPSTREAM_KEY
        WEB_SITE=http://www.freetype.org/
         ENTERED=20010922
      LICENSE[0]=GPL
        KEYWORDS="fonts graphics libs"
           SHORT="free, quality, portable font engine"
cat << EOF
