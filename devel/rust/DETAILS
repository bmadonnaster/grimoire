           SPELL=rust
          SPELLX=rustc
         VERSION=1.20.0
          SOURCE=$SPELLX-$VERSION-src.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELLX-$VERSION-src
           TMPFS=off
   SOURCE_URL[0]=https://static.rust-lang.org/dist/$SOURCE
         SOURCE2=$SOURCE.asc
  SOURCE2_URL[0]=${SOURCE_URL[0]}.asc
      SOURCE_GPG=rust.gpg:$SOURCE.asc:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
# Try to preempt the build-time download for x86-64 at least.
# Also: Is this still free/open source when we pull a binary?
if [[ ${SMGL_COMPAT_ARCHS[1]} == x86_64 ]]; then
           BDATE=2017-07-20
         SOURCE3=$SPELL-std-1.19.0-x86_64-unknown-linux-gnu.tar.gz
  SOURCE3_URL[0]=https://static.rust-lang.org/dist/$BDATE/$SOURCE3
         SOURCE4=$SOURCE3.asc
  SOURCE4_URL[0]=${SOURCE3_URL[0]}.asc
  SOURCE4_IGNORE=signature
     SOURCE3_GPG=rust.gpg:$SOURCE4:UPSTREAM_KEY
         SOURCE5=$SPELLX-1.19.0-x86_64-unknown-linux-gnu.tar.gz
  SOURCE5_URL[0]=https://static.rust-lang.org/dist/$BDATE/$SOURCE5
         SOURCE6=$SOURCE5.asc
  SOURCE6_URL[0]=${SOURCE5_URL[0]}.asc
  SOURCE6_IGNORE=signature
     SOURCE5_GPG=rust.gpg:$SOURCE6:UPSTREAM_KEY
         SOURCE7=cargo-0.20.0-x86_64-unknown-linux-gnu.tar.gz
  SOURCE7_URL[0]=https://static.rust-lang.org/dist/$BDATE/$SOURCE7
         SOURCE8=$SOURCE7.asc
  SOURCE8_URL[0]=${SOURCE7_URL[0]}.asc
  SOURCE8_IGNORE=signature
     SOURCE7_GPG=rust.gpg:$SOURCE8:UPSTREAM_KEY
fi
        WEB_SITE=https://www.rust-lang.org/
         ENTERED=20130529
      LICENSE[0]=MIT
           SHORT="safe, concurrent, practical language"
cat << EOF
Rust is a safe, concurrent, practical programming language.
