default_pre_build    &&
cd $SOURCE_DIRECTORY &&
if [[ -n $SOURCE3 ]]; then
  verify_file 3 &&
  mkdir -p build/cache/$BDATE &&
  ln -sv "$SOURCE_CACHE/$SOURCE3" build/cache/$BDATE
fi &&
if [[ -n $SOURCE5 ]]; then
  verify_file 5 &&
  mkdir -p build/cache/$BDATE &&
  ln -sv "$SOURCE_CACHE/$SOURCE5" build/cache/$BDATE
fi
if [[ -n $SOURCE7 ]]; then
  verify_file 7 &&
  mkdir -p build/cache/$BDATE &&
  ln -sv "$SOURCE_CACHE/$SOURCE7" build/cache/$BDATE
fi
