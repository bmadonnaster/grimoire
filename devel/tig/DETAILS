   SOURCE_URL[0]=https://github.com/jonas/${SPELL}/archive/${SOURCE}
     SOURCE_HASH=sha512:6ff97be91bbeffa3bffcb3ec17be2ec2e8ee24fb047e0f969749a547ad565f3087e4249c3b1e0c47f7ce5aa897fbf63ca3b84d153f7fadfd7f5cfb4a0f58ca67
