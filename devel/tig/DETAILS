           SPELL=tig
         VERSION=2.3.3
          SOURCE="${SPELL}-${VERSION}.tar.gz"
   SOURCE_URL[0]=https://github.com/jonas/${SPELL}/archive/${SOURCE}
     SOURCE_HASH=sha512:f5bfeb1db2e0ea2486e01eccd341f855826cb793bcf391daa23d39be70afa1a9e7988b7a6c164a69b40da64b755f6a63a723ce5490dfcd8e03fbc415bdca0aa6
