   SOURCE_URL[0]=https://github.com/jonas/${SPELL}/archive/${SOURCE}
     SOURCE_HASH=sha512:36a5c4338ed0a44494380f010cee06ba1db91e01136f3b9ba94b05de0969a0ddeceba7a664081b1f01ceacf423e04a5e0589d8e698365802128f5491af26ec5f
