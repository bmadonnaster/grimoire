           SPELL=tig
         VERSION=2.2.2
          SOURCE="${SPELL}-${VERSION}.tar.gz"
   SOURCE_URL[0]=https://github.com/jonas/${SPELL}/archive/${SOURCE}
     SOURCE_HASH=sha512:bf3678ce066027aba79eb0dcf298e2ee611ba289a6382e72723997b37b2d5aad0abd77f41642a576ce403dc4855a7c53e5bdeb0f059ee89e43e26627f5681d85
