   SOURCE_URL[0]=https://download.gnome.org/sources/libxslt/${VERSION%.??}/$SOURCE
        DOC_DIRS=""
      LICENSE[0]=BSD
        WEB_SITE=https://xmlsoft.org/
         ENTERED=20011010
