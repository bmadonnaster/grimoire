           SPELL=fish
         VERSION=3.3.0
          SOURCE=$SPELL-$VERSION.tar.xz
   SOURCE_URL[0]=https://github.com/fish-shell/fish-shell/releases/download/$VERSION/$SOURCE
     SOURCE_HASH=sha256:a4215e4cab2a5b101b0b8843720bda3c7eb98e8a14dca0950b8ef17e94282faa:UPSTREAM_HASH
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$SPELL-$VERSION"
        WEB_SITE=http://fishshell.com/
         ENTERED=20051220
      LICENSE[0]=GPL
        KEYWORDS="console"
           SHORT="fish is a user friendly command line shell"
