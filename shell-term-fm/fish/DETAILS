source "${GRIMOIRE}/CMAKE_FUNCTIONS"
           SPELL=fish
         VERSION=3.3.1
          SOURCE=$SPELL-$VERSION.tar.xz
   SOURCE_URL[0]=https://github.com/fish-shell/fish-shell/releases/download/$VERSION/$SOURCE
     SOURCE_HASH=sha256:b5b4ee1a5269762cbbe993a4bd6507e675e4100ce9bbe84214a5eeb2b19fae89:UPSTREAM_HASH
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$SPELL-$VERSION"
        WEB_SITE=http://fishshell.com/
         ENTERED=20051220
      LICENSE[0]=GPL
        KEYWORDS="console"
           SHORT="fish is a user friendly command line shell"
cat << EOF
