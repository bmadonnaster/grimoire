          SOURCE=${SPELL}-${VERSION}.tar.gz
         SOURCE2=${SOURCE}.sig
   SOURCE_URL[0]=https://git.sr.ht/~sircmpwn/${SPELL}/refs/${VERSION}/${SOURCE}
