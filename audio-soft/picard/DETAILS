         VERSION=2.2.3
          SOURCE=${SPELL}-${VERSION}.tar.gz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${SPELL}-release-${VERSION}
   SOURCE_URL[0]=http://ftp.musicbrainz.org/pub/musicbrainz/picard/$SOURCE
   SOURCE_URL[0]=ftp://ftp.eu.metabrainz.org/pub/musicbrainz/picard/$SOURCE
