   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:85a83afabba05157a72b8ee3e2fc38fbd190ddc3047b4251bba1333aa014d943cf9ac316c27f8a4a4e3edc22513872771f52a6b1ff18e5cb3182d510da1c711a
