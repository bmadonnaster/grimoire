           SPELL=psutil
         VERSION=5.4.8
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:66990d006680142c309874624882ba7612cdef99f44cfed07ba5371ba20e1c978e42364248dba09fce053d78a62630061d1995662f9b965492ea2261eda83ea8
