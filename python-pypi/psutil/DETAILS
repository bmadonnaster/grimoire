         VERSION=5.6.6
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:c03ff2c0b06012d19c2944ffa0909553965a23286de9463dec64cb4b5f1ba841614ab730e19849a0278b805b8426f4bd3bfe0bac39930e830db17007026565dc
