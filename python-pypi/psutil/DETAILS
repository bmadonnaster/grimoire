         VERSION=5.3.0
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:d738ee98193f713ea221a945231239002af579a5a5b525c7dc702f7894cdfd404b00641386451298780f3a47177beaeeaf51d4225fd7775456dd3522aa7dcc22
