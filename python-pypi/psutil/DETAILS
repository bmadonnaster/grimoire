   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:a36c7b3d622702179329ec67d6ffe09958cf2098f28d0aba74e3b66a8872d91936020e46eb2c5b36644fbef978ecfa5dda77d7c8951abab9da1864a59e328efe
