         VERSION=5.4.7
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:86ea6e52f551f5de49403cc38fc6d3123db3e5328c60a19bedc4757cfae88b9a2294ca44f5a6dab2c2c3714cacbab35671c16c44fe4dd8ce86f7a68ff74174e9
