   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:163e856d77f46caef2f4ff6d969344ab079890efdeecc84c2d70b40c8b1576376655424317a234cbb62c8f229fff90d246c878fb16018e414eca637f669bf4bb
