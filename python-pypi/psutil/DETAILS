         VERSION=5.6.3
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:207fb14a26e078829534d9241db02a90ffa827d424d923527c488ffb8da028bf568744a1813f289455fe496c30480c92aa8187f984b1aa7b1fe6fe0dc1419810
