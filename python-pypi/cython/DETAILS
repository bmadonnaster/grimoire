           SPELL=cython
          SPELLX=Cython
         VERSION=0.28.2
          SOURCE=${SPELLX}-${VERSION}.tar.gz
   SOURCE_URL[0]=https://pypi.org/packages/source/C/${SPELLX}/${SOURCE}
     SOURCE_HASH=sha512:7d09d1fdc10f96eb6121657b71e059c38c479b4ca180965a8e5adb89ac0f9be50260ea46197c45e8ddc7c1339b9b88c140dac2c4a7a49e1943fb4bf24a73d082
