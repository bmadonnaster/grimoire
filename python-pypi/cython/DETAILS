   SOURCE_URL[0]=https://github.com/cython/cython/archive/${VERSION}.tar.gz
     SOURCE_HASH=sha512:52490d0b5355e13cbe586830f763173d7556cf3d79d79192ca75138b1190e7a4c1f3feeb0568349802ef3b97300c3805f54eef5ffd73a5180d68f023ac2a44cd
