           SPELL=cython
         VERSION=0.29.24
          SOURCE=${SPELL}-${VERSION}.tar.gz
   SOURCE_URL[0]=https://github.com/cython/cython/archive/${VERSION}.tar.gz
     SOURCE_HASH=sha512:387a827f3cb7428248019744e3ed606a133d98ac882b83624d24ae0fdf8a8f2334978555d7d84e5cc4adaf715c541fef64e45bcbdda85ca33473c21f99360be8
