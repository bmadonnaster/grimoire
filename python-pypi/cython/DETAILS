           SPELL=cython
          SPELLX=Cython
         VERSION=0.29.5
          SOURCE=${SPELLX}-${VERSION}.tar.gz
   SOURCE_URL[0]=https://pypi.org/packages/source/C/${SPELLX}/${SOURCE}
     SOURCE_HASH=sha512:4bbf54b248b9a08b9118d751a6bb5dbb81ae7a8238c8a3124d94bf7be67f15b68820e6ce5470903fb5d5ff5c3c213e1580f1c57a8cda33b356263f9ab46e8851
