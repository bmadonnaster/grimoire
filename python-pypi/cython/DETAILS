           SPELL=cython
          SPELLX=Cython
         VERSION=0.29.9
          SOURCE=${SPELLX}-${VERSION}.tar.gz
   SOURCE_URL[0]=https://pypi.org/packages/source/C/${SPELLX}/${SOURCE}
     SOURCE_HASH=sha512:17ac97eaa01e3c6cd9c7d597822ad25ef5cec2354055304a2999a0a0d969fa0513640470260cf0d88c8d9a7c331f02d1ebf144245a6c45de47e26e7fa73b0ae1
