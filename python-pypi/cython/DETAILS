   SOURCE_URL[0]=https://pypi.org/packages/source/C/${SPELLX}/${SOURCE}
     SOURCE_HASH=sha512:df8430bb89816df84abfc6d59bee8e50f4117e8e82b9c82a889afca390a42a1569a9b5dcde76f40e7c29d728489af34c6fe0cf359912f7b74ac0d014af2171c3
