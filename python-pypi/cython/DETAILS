           SPELL=cython
         VERSION=0.29.16
          SOURCE=${SPELL}-${VERSION}.tar.gz
   SOURCE_URL[0]=https://github.com/cython/cython/archive/${VERSION}.tar.gz
     SOURCE_HASH=sha512:8f7a05ad86d557d774b2de5ec77fa9bc2600500555b607482899ade2b8abb2ebb60fc1b944f355b37663e30de09d53bcaa925f888b4128f4b0345a1cad9e2923
