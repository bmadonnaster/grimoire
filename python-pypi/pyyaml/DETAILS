   SOURCE_URL[0]=https://pyyaml.org/download/${SPELL}/${SOURCE}
     SOURCE_HASH=sha512:93642286d0317e2fe970632c36d38ce6030f7cabcf971f28e3a1054f07390fcee5baaf7f167e7c9690dbd7b2adc61f5b7d75a218ace0abca34ff8815486cfdd7
