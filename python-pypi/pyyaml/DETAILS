           SPELL=pyyaml
         VERSION=5.2
          SOURCE="${SPELL}-${VERSION}.tar.gz"
   SOURCE_URL[0]=https://github.com/yaml/pyyaml/archive/$VERSION.tar.gz
     SOURCE_HASH=sha512:8d8ec87cf5ce7f4299da5a7ffbaee60372bf1606d6e527f0897cde7cea3234100502767c0975027151bc99549d9d8edf243c003ea08948670f367e9778abdc89
