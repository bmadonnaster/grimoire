         VERSION=5.3
          SOURCE="${SPELL}-${VERSION}.tar.gz"
   SOURCE_URL[0]=https://github.com/yaml/pyyaml/archive/$VERSION.tar.gz
     SOURCE_HASH=sha512:51e07dd80d4e2758429c18e55d7664b8f8e3fb2129f74655cefe3e13ac2838148432f2736d5304e107c9241bcf3e556e313aa0e71560e59df8c49bdfffd0fb9d
