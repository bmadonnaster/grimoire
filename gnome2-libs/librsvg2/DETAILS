           SPELL=librsvg2
if [[ "${LIBRSVG2_BRANCH}" == "devel" ]]; then
         VERSION=2.49.3
     SOURCE_HASH=sha512:161a98f70ce63e9db7b61ae51c8551db792ec5d2b68d04f9724f986060de244e74598326f69ea80376cd01a539c2a8ee02bdc94db1233880b9696655548a4b4e
