           SPELL=dbi
          SPELLX=DBI
         VERSION=1.640
          SOURCE=$SPELLX-$VERSION.tar.gz
   SOURCE_URL[0]=$PERL_CPAN_URL/CPAN/authors/id/T/TI/TIMB/$SOURCE
     SOURCE_HASH=sha512:d2d08d1caa5779d3673ccddd9836fb4edf42909880cdb7c8554d09527bd6d44faf410cb1587a0ad880b0677abf21fd3ef71458b889d5a1bb5cecda020bdb4bb0
