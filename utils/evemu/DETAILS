           SPELL=evemu
         VERSION=2.7.0
          SOURCE=$SPELL-$VERSION.tar.xz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
        WEB_SITE=https://www.freedesktop.org/wiki/Software/$SPELL
   SOURCE_URL[0]=https://www.freedesktop.org/software/$SPELL/$SOURCE
         SOURCE2=$SOURCE.sig
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sig
      SOURCE_GPG=freedesktop.gpg:$SOURCE.sig:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
      LICENSE[0]=GPL
         ENTERED=20181210
        KEYWORDS=""
           SHORT="Kernel input device emulatio"
cat << EOF
