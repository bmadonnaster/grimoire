   SOURCE_URL[0]=https://brick.kernel.dk/snaps/${SOURCE}
     SOURCE_HASH=sha512:05b85755411da5ccf620a8f4cc53347de01a605347ccc40769e850dc553abe9472231bd059e8ffe8d59681d7ed4291c7cce38b0f2109afbc390fbed6329f3a31
