           SPELL=rlwrap
         VERSION=0.44
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/hanslub42/${SPELL}/releases/download/${VERSION}/${SOURCE}
     SOURCE_HASH=sha512:b7bbec5a04ae3588d467d985c0bd7d4a6a91014c7f3a9fca8eb184f9d699e2463fc334c9eb336690427efce3ad62cd2a5b59b892790643a7a3260b6218e4a145
