          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]="https://sourceware.org/pub/${SPELL}/${SOURCE}"
        WEB_SITE=http://www.bzip.org/
