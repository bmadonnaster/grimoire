# special functions for projects which use the NetSurf buildsystem

default_build () {
  make -j "$MAKE_NJOBS" \
       PREFIX="${INSTALL_ROOT}/usr" \
       COMPONENT_TYPE=lib-shared \
       $OPTS
}

default_install () {
  make -j "$MAKE_NJOBS" \
       PREFIX="${INSTALL_ROOT}/usr" \
       COMPONENT_TYPE=lib-shared \
       install
}
