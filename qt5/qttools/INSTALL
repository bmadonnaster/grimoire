default_install &&

for exec in assistant designer lconvert linguist lrelease lupdate pixeltool \
  qcollectiongenerator qdbus qdbusviewer qdoc qhelpconverter qhelpgenerator \
  qtattributionsscanner qtdiag qtpaths qtplugininfo; do
  patchelf --set-rpath /opt/qt5/lib "${INSTALL_ROOT}/opt/qt5/bin/$exec"
done
