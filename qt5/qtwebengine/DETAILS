           SPELL=qtwebengine
          SPELLX=${SPELL}-everywhere-opensource-src
         VERSION=5.15.4
         VX=${VERSION%-*}
     SOURCE_HASH=sha512:
  SECURITY_PATCH=17
          SOURCE=$SPELLX-${VERSION}.tar.xz
      SOURCE_URL=$QT_URL/qt/${VX%.?}/${VERSION}/submodules/$SOURCE
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-everywhere-src-${VERSION}
        WEB_SITE=https://www.qt.io
         ENTERED=20141214
           TMPFS=off
      LICENSE[0]=LGPL3
        KEYWORDS=""
           SHORT="browser engine"
